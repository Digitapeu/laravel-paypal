{
  "name": "digitapeu/paypal",
  "type": "library",
  "description": "Laravel plugin For Processing Payments Through Paypal Express Checkout. Can Be Used Independently With Other Applications.",
  "keywords": ["http", "rest", "web service", "paypal", "laravel paypal"],
  "license": "MIT",
  "authors": [
    {
      "name": "Raza Mehdi",
      "email": "srmk@outlook.com"
    },
    {
      "name": "Daniel Tamas",
      "email": "daniel@digtap.eu"
    }
  ],
  "autoload": {
    "psr-4": {
      "Digitap\\PayPal\\": "src/"
    }
  },
  "require": {
    "guzzlehttp/guzzle": "~6.0",
    "illuminate/support": "~5.1|~5.2|~5.3|~5.4|~5.5|~5.6|~5.7|~5.8|~6.0",
    "nesbot/carbon": "~1.0|~2.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Digitap\\PayPal\\Providers\\PayPalServiceProvider"
      ],
      "aliases": {
        "PayPal": "Digitap\\PayPal\\Facades\\PayPal"
      }      
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
